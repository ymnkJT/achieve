<div class="container">
  <h2>ユーザー</h2>
  <% @users.each do |user| %>
    <div class="container-child col-md-2 col-sm-4 col-xs-6">
      <div class="prof_img">
        <%= profile_img(user) %>
      </div>
      <%= user.email %>
      <%= render 'follow_form', user: user %>
    </div>
  <% end %>
</div>
